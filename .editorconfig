[*.{fs,fsx}]
max_line_length=100

fsharp_max_value_binding_width=150
fsharp_max_infix_operator_expression=150
fsharp_max_dot_get_expression_width=150
fsharp_max_array_or_list_number_of_items=10
fsharp_max_array_or_list_width=80
fsharp_max_function_binding_width=20

fsharp_multi_line_lambda_closing_newline=true
fsharp_multiline_block_brackets_on_same_column=true